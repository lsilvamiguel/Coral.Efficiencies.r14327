dist: 
	@mkdir -p .tmp/Dtp && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/Dtp/ && ( cd `dirname .tmp/Dtp` && $(TAR) Dtp.tar Dtp && $(GZIP) Dtp.tar ) && mv `dirname .tmp/Dtp`/Dtp.tar.gz . && rm -rf .tmp/Dtp

